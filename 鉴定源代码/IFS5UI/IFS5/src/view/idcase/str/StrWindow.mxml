	<ns1:ListPager id="pager" right="10" bottom="10"
				   creationComplete="pager.addEventListener(ListPager.PAGE_INDEX_CHANGE_EVENT,button1_clickHandler);locObj.allListPager=pager;"
				   pageSize="27">
	</ns1:ListPager>
