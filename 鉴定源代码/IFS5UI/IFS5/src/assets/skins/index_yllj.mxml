<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:d="http://ns.adobe.com/fxg/2008/dt"
		width="100" height="18">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage d:userLabel="首 页" x="0" y="0" smooth="true"
				   source="@Embed('/assets/images/index/预留链接.png')">
		<s:filters>
			<s:DropShadowFilter includeIn="over" alpha="0.5" angle="45.0" blurX="2" blurY="2"
								color="#2B4381" distance="2" hideObject="false" inner="false"
								knockout="false" quality="2" strength="1"/>
			<s:DropShadowFilter includeIn="down" alpha="0.5" angle="45.0" blurX="4.0" blurY="4.0"
								color="#5380D0" distance="4.0" hideObject="false" inner="true"
								knockout="false" quality="2" strength="1"/>
		</s:filters>
	</s:BitmapImage>
</s:Skin>
